                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </HBox>
                                    <TabPane fx:id="tabPane" focusTraversable="false" minHeight="0.0" minWidth="0.0" styleClass="project-tabs" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
